public namespace e6;
import common -;
import meta -;

metafunction once{f} metafunction{x} f{x};
metafunction twice{f} metafunction{x} f{f{x}};
metafunction incr{x} add{x, 1};

function void t1()
{
  int x = apply{once{incr}, 5};
  println(x);
  int y = apply{twice{incr}, 5};
