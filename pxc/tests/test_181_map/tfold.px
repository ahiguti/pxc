public namespace tfold;
public import common -;

public struct foo {
  public int x;
  public int y;
  public int z;
}

{
  foo a;
  a.x = 1;
  a.y = 2;
  a.z = 3;
  const v = fold_list{operator::add}(0, a);
  println(v);
}

{
  const x = make_farray{int}(10, 20, 30, 40);
