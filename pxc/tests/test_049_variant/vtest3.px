public multithreaded struct foo(int x) {
  int val = x;
  public function void add(int x) { val += x; }
  public function int get() const { return val; }
  public function int func1(int mutable& x, long const& y) {
    x = val;
    val += n::static_cast{int, long}(y);
    return val;
  }
