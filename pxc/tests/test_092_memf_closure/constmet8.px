public namespace constmet8;
import common -;

import meta m;

function {f} m::ret_type{f} apply(m::arg_type{f, 0} const& x)
{
  return f(x);
}

struct foo {
  public int x;
  public function int m(int y) const {
    function int nf(int z) {
      return x + z;
    }
    return apply{nf}(y);
  }
}

function void t1() {
  ptr{foo} v = make_ptr(foo(10));
  println(v->m(20));
}

t1();
