public namespace upcast;
import common -;

interface ifoo {
  public function void m1();
}

interface ibar <ifoo> {
  public function void m2();
}

struct baz <ibar> {
  public function void m1() { println("m1"); }
  public function void m2() { println("m2"); }
}

function void t1()
{
  baz bz;
  ibar mutable& b = bz;
  b.m2();
  ifoo mutable& f = b;
  f.m1();
}

t1();
