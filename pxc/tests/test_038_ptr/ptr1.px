namespace ptr1;
import common -;
import meta m;

mtvaluetype struct foo(int x_, string const& s_) {
  int x = x_;
  string s = s_;
}

function {con} void t1() {
  int const x = 30;
  string const s = "abc";
  const f = m::apply{m::symbol{con}, foo, m::list{m::list{foo, 0}}}(foo(x, s));
  print(meta::full_string{meta::typeof{f}});
  print(" ");
  print(f->x);
  print(" ");
  print(f->s);
  print(" ");
  println(f);
}

