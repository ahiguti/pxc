public multithreaded interface ifoo {
  public function int get_v() const;
}

public multithreaded struct sfoo < ifoo > {
  public int v;
  public function int get_v() const { return v; }
}

public multithreaded struct my_thread_obj(int x0, int y0, tptr{ifoo} p0,
  io::io sys0)
{
  int x = x0;
  int y = y0;
  io::io sys = sys0;
  tptr{ifoo} p = p0;
  public function void main() {
    const stdout = sys.stdout();
    io::standard::file_println(stdout, "my thread main");
    io::standard::file_println(stdout, x);
    io::standard::file_println(stdout, y);
    io::standard::file_println(stdout, p->get_v());
  }
}

function void test1()
{
  tptr{sfoo} p = make_tptr(sfoo(3));
