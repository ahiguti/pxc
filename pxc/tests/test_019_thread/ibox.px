namespace ibox;
import common -;


interface ifoo {
  function int get_x() const;
  function int get_y() const;
}

struct foo < ifoo > {
  int x;
  int y;
  function int get_x() const { return x; }
  function int get_y() const { return y; }
}

struct bar < ifoo > {
  int x;
  int y;
  int z;
  int w;
  function int get_x() const { return x; }
  function int get_y() const { return y; }
}

function void test1()
{
  ptr{ifoo} p1 = make_ptr{foo}(foo(5, 10));
  println(p1->get_y());
  ptr{ifoo} p2 = make_ptr{bar}(bar(1, 2, 3, 4));
  println(p2->get_y());
}

test1();
