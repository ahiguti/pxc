namespace dia2;
import common -;

interface i1 {
  function int f1(int x);
}

interface i2 <i1> {
  function int f2(int x);
}

interface i3 <i1> {
  function int f3(int x);
}

struct foo <i2, i3> {
  function int f1(int x) { return x + 1; }
  function int f2(int x) { return x + 2; }
  function int f3(int x) { return x + 3; }
}

function void t1() {
  foo f1;
  i2 mutable& v2 = f1;
  i1 mutable& v1 = v2;
  int r = v1.f1(5);
  println(r);
}

t1();
