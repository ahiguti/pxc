namespace memtmpl;
import common -;
import numeric::cast -;

threaded struct foo {
  int v;
  function {t} void m1(t x) {
    threaded function {s} void add(s w) {
      v += numeric_cast{int, s}(w);
    }
    add(x);
    // v += x;
  }
}

foo f;
int iv = 100;
long lv = 200;
ulong ulv = 300;
f.m1(iv);
f.m1(lv);
f.m1(ulv);
println(f.v);
