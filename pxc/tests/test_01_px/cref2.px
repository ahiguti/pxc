function int f1(ptr{int} mutable& x, cptr{int} mutable& y)
{
  *x += 1;
  *y += 1; // can not modify ...
  return *x + *y;
}

function void test1()
{
  ptr{int} x = int(0);
  *x = 3;
  ptr{int} y = int(0);
  *y = 4;
  println(f1(x, y));
  println(*x);
}

test1();

