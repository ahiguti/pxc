namespace cref2;
import test_common "";

import io;

function int f1(ptr{int}& x, cptr{int}& y)
{
  *x += 1;
  *y += 1; // can not modify ...
  return *x + *y;
}

function void test1()
{
  ptr{int} x = int(0);
  *x = 3;
  ptr{int} y = int(0);
  *y = 4;
  io::println(f1(x, y));
  io::println(*x);
}

test1();

