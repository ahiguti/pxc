
namespace list5;
import io;
import list "";

function {t, op, zero} t fold(list{t} xs) {
  list{t} p = xs;
  t v = zero;
  while (case p.value) {
    v = op(v, p.value->head);
    p = p.value->rest;
  }
  return v;
}

function {t} t add(t x, t y)
{
  return x + y;
}

function {t} t mul(t x, t y)
{
  return x * y;
}

macro makeintcons makecons{int};
macro intlist list{int};

intlist xs = makeintcons(3, makeintcons(4, makeintcons(5, intlist())));
io::println(fold{int, add{int}, 0}(xs));
io::println(fold{int, mul{int}, 1}(xs));

