import test_common "";

namespace vari5;
import io;

union foo {
  int intval;
  string strval;
}

function void println_foo(foo v)
{
  if (case v.intval) {
    io::println(v.intval);
  } else if (case v.strval) {
    io::println(v.strval);
  }
}

function bool hoge(bool v)
{
  return v;
}

foo v;
v.intval = 3;
bool b = hoge(case v.intval);
if (b) {
  io::println("true");
} else {
  io::println("false");
}
