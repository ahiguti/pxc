
namespace erat1;
import test_common "";

function ptr{vector{uint}} eratosthenes(int x)
{
  vector{uint} uiv;
  ptr{vector{uint}} ret = ptr(uiv);
  vector{uint} sieve;
  sieve.resize(x);
  sieve[1] = 1;
  uint i;
  for (i = 2; i < x; ++i) {
    if (sieve[i] == 0) {
      ret->push_back(i);
      uint j;
      for (j = i * i; j < x; j += i) {
	sieve[j] = 1;
      }
    }
  }
  return ret;
}

ptr{vector{uint}} arr = eratosthenes(100000);
uint i;
for (i = 0; i < arr->size(); ++i) {
  io::println((*arr)[i]);
}

