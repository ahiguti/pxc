
namespace erat1;
import common "";

function ptr{vector{size_t}} eratosthenes(size_t x)
{
  vector{size_t} uiv;
  ptr{vector{size_t}} ret = ptr{vector{size_t}}(uiv);
  vector{size_t} sieve;
  sieve.resize(x, 0);
  sieve[1] = 1;
  size_t i;
  for (i = 2; i < x; ++i) {
    if (sieve[i] == 0) {
      ret->push_back(i);
      size_t j;
      for (j = i * i; j < x; j += i) {
	sieve[j] = 1;
      }
    }
  }
  return ret;
}

ptr{vector{size_t}} arr = eratosthenes(100000);
size_t i;
for (i = 0; i < arr->size(); ++i) {
  println((*arr)[i]);
}

