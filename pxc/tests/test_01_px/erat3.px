
namespace erat3;
import test_common "";

macro uivec vector{uint};
macro uivecref ptr{uivec};

function uivecref eratosthenes(uint x)
{
  uivec sieve;
  sieve.resize(x);
  sieve[1] = 1;
  uivecref ret = ptr(uivec());
  uint i;
  for (i = 2; i < x; ++i) {
    if (sieve[i] == 0) {
      ret->push_back(i);
      uint j;
      for (j = i * i; j < x; j += i) {
	sieve[j] = 1;
      }
    }
  }
  return ret;
}

uivecref arr = eratosthenes(100000);
uint i;
for (i = 0; i < arr->size(); ++i) {
  io::println((*arr)[i]);
}

