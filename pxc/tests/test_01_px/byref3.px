
namespace byref3;
import test_common "";
import array;

function void func_byref(int x, int& y)
{
  y += x;
}

function void test1()
{
  int a = 30;
  vector{ptr{int}} b;
  array::resize(b, 10, ptr{int}(3));
  int x = 3;
  b[0] = ptr{int}(x);
  int y = 3;
  b[1] = ptr{int}(y);
  func_byref(*b[0], *b[1]);
  println(*b[1]);
}

test1();

