namespace filetest;
import io;

function void ft1()
{
  if ((int r = io::main.stat(string fn = "hoge.txt", io::statbuf stbuf)) != 0) {
    io::print("failed to stat: ");
    io::println(fn);
  } else {
    io::println(stbuf.st_size);
  }
}

function void ft2()
{
  io::main.read_file("hoge.txt", string s);
  io::print(s);
}

ft1();
ft2();

