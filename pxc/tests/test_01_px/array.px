
namespace array;

extern "array::length" function {t} ulong length(vector{t} a);
extern "array::empty" function {t} bool empty(vector{t} a);
extern "array::resize" function {t} void resize(vector{t}& a, ulong len);
extern "array::clear" function {t} void clear(vector{t}& a);
extern "array::push" function {t} void push(vector{t}& a, t v);
extern "array::pop" function {t} t pop(vector{t}& a);

extern "fdecl" inline
namespace array {
template <typename T> unsigned long long length(const pxcrt::vector<T>& a)
{ return a.size(); };
template <typename T> bool empty(const pxcrt::vector<T>& a)
{ return a.empty(); };
template <typename T> void resize(pxcrt::vector<T>& a, unsigned long long len)
{ a.resize(len); }
template <typename T> void clear(pxcrt::vector<T>& a)
{ a.clear(); }
template <typename T> void push(pxcrt::vector<T>& a, const T& v)
{ a.push_back(v); }
template <typename T> T pop(const pxcrt::vector<T>& a)
{ T r = a.at(a.size() - 1); a.pop_back(); return r; }
};

