
import array;
import io;
namespace erat2;

function ref{vector{uint}} eratosthenes(int x)
{
  ref{vector{uint}} ret;
  vector{uint} sieve;
//  array::resize(sieve, x);
// error!
  sieve[1] = 1;
  for (uint i = 2; i < x; ++i) {
    if (sieve[i] == 0) {
      array::push(*ret, i);
      for (uint j = i * i; j < x; j += i) {
	sieve[j] = 1;
      }
    }
  }
  return ret;
}

ref{vector{uint}} arr = eratosthenes(100000);
for (uint i = 0; i < array::length(*arr); ++i) {
  io::println((*arr)[i]);
}

