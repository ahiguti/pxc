
namespace foldl2;
import test_common "";

vector{int} a;
a.resize(101, 0);
uint i;
for (i = 0; i < a.size(); ++i) {
  a[i] = i;
}
function int add(int x, int y) {
  return x + y;
}
int z = foldl(0, a);
io::println(z);

function {t} int foldl(int v, t m) {
  int r = v;
  uint i;
  for (i = 0; i < m.size(); ++i) {
    r = add(r, m[i]);
  }
  return r;
}

