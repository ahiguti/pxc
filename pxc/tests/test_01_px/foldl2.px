
namespace foldl2;
import common "";
import numeric::cast "";

vector{int} a;
a.resize(101, 0);
size_t i;
for (i = 0; i < a.size(); ++i) {
  a[i] = numeric_cast{int, size_t}(i);
}
function int add(int x, int y) {
  return x + y;
}
int z = foldl(0, a);
println(z);

function {t} int foldl(int v, t m) {
  int r = v;
  size_t i;
  for (i = 0; i < m.size(); ++i) {
    r = add(r, m[i]);
  }
  return r;
}

