
namespace myarray;
import common -;

function extern "myarray::length" {t} ulong length(vector{t} const& a);
function extern "myarray::empty" {t} bool empty(vector{t} const& a);
function extern "myarray::resize" {t} void
  resize(vector{t}& a, ulong len, t v);
function extern "myarray::clear" {t} void clear(vector{t}& a);
function extern "myarray::push" {t} void push(vector{t}& a, t v);
function extern "myarray::pop" {t} t pop(vector{t}& a);

extern "fdecl" inline
namespace myarray {
template <typename T> unsigned long long length(const array::pxcvector<T>& a)
  { return a.size(); };
template <typename T> bool empty(const array::pxcvector<T>& a)
  { return a.empty(); };
template <typename T> void resize(array::pxcvector<T>& a,
  unsigned long long len, T v)
  { a.resize(len, v); }
template <typename T> void clear(array::pxcvector<T>& a)
  { a.clear(); }
template <typename T> void push(array::pxcvector<T>& a, T v)
  { a.push_back(v); }
template <typename T> T pop(const array::pxcvector<T>& a)
  { T r = a.at(a.size() - 1); a.pop_back(); return r; }
};

