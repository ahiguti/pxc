  t pop_back(vector{t}& vec);

extern "fdef" inline
namespace array {
template <typename T> void
push_back(array::pxcvector<T>& v, const T& x)
{
  v.push_back(x);
}
template <typename T> T
pop_back(array::pxcvector<T>& v)
{
  if (v.empty()) {
    T rv = T();
    return rv;
  } else {
    T rv = v[v.size() - 1];
    v.pop_back();
    return rv;
  }
}
}; // namespace array

