import common "";

namespace tmplup;
import meta::builtin m;

function void test1() {
  int v = 0;
  int w = 0;
  function {f} m::argtype{f, 0} apply() {
    return f(v * v);
  }
  function int foo(int x) {
    return x + w;
  }
  v = 10;
  w = 20;
  int z = apply{foo}();
  println(z);
}

test1();

