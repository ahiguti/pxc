namespace interf_tmpl;
import common -;
import numeric::cast -;

threaded interface ifoo {
  function int m(int x);
}

threaded struct {t} foo : ifoo : {
  t data;
  function int m(int x) {
    return numeric_cast{int, t}(data + x);
  }
}

long q = 10;
ptr{foo{int}} r = foo(q); // internal error
ptr{ifoo} ir = r;
int v = ir->m(100);
println(v);
