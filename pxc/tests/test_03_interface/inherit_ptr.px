import test_common "";

namespace inherit_ptr;
import io;

interface ifoo {
  function int m(int x);
}

struct foo : ifoo : {
  function int m(int x) {
    return x * 2;
  }
}

function void test1()
{
  ptr{ifoo} p = ptr{foo}(foo()); // boxing from foo to ptr{ifoo}
  int r = p->m(100);
  io::println(r);
}

test1();
