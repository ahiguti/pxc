import common -;
namespace itmpl1;

interface comparable {
  function int compare(foo const& x, foo const& y) const;
}

struct compare_object < comparable > {
  function int compare(foo const& x, foo const& y) const {
    if (x.v < y.v) {
      return -1;
    } else if (x.v > y.v) {
      return 1;
    }
    return 0;
  }
}

struct foo {
  int v;
}

function void test1()
{
  foo x = foo(10);
  foo y = foo(20);
  ptr{compare_object} ro = to_ptr{compare_object}(compare_object());
  ptr{comparable} c = ro;
  int v = c->compare(x, y);
  println(v);
}

test1();

