namespace itmpl0;
import io;

interface {t} comparable {
  function int compare(t x, t y) const;
}

struct compare_object {
  function int compare(foo x, foo y) const {
    if (x.v < y.v) {
      return -1;
    } else if (x.v > y.v) {
      return 1;
    }
    return 0;
  }
}

struct foo {
  int v;
}

foo x = foo(10);
foo y = foo(20);
ptr{compare_object} c = ptr(compare_object());
int v = c->compare(x, y);
io::println(v);
