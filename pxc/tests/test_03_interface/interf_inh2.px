namespace interf_inh2;
import common -;

interface ifoo {
  function void foo();
}

interface ibar <ifoo> {
  function void bar();
}

struct fubar <ibar> {
  function void bar() {
    println("bar");
  }
  function void foo() {
    println("foo");
  }
}

function void f1(ifoo& f)
{
  f.foo();
}

function void t1()
{
  fubar x;
  f1(x);
}

t1();
