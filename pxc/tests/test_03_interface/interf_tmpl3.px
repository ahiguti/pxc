
namespace interf_tmpl;
import io;

interface ifoo {
  function int m(int x);
}

struct {t} foo : ifoo : {
  function int m(int x) {
    return data + x;
  }
  t data;
}

function void test1()
{
  int q = 10;
  ptr{foo{int}} r = ptr(foo(q));
  ptr{ifoo} ir = r;
  int v = ir->m(100);
  io::println(v);
}

test1();

