
namespace deref2;
import common -;

metafunction tm tree_map{int, int};
metafunction tmr tree_map_range{int, int};

function int t1(tmr const& ra)
{
  mutable v = *ra;
  v *= 2;
  return v;
}

function tm f1()
{
  tree_map{int, int} m;
  m[3] = 55;
  return m;
}

function void test()
{
  mutable ra = f1();
  int v = t1(ra);
  println(v);
}

test();
