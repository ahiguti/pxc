namespace ser2;
import common -;
import text::serialize::compact_pod_array -;

public struct foo {
  public int x;
  public int y;
  public string z;
  public varray{uint} v;
}

{
  foo v;
  v.x = 10;
  v.y = 20;
  v.z = "abc";
  for (const i: 0 .. 100u) {
    v.v.push_back(i);
  }
  println(v);
  string s = to_string_compact_pod_array(v);
  println(s);
  const v2 = from_string_compact_pod_array{foo}(s);
  println(v2);
}
