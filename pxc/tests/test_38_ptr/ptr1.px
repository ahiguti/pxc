namespace ptr1;
import common -;

mtvaluetype struct foo(int x_, string const& s_) {
  int x = x_;
  string s = s_;
}

function {t} void t1() {
  int const x = 30;
  string const s = "abc";
  t const f = t(foo(x, s));
  print(meta::common::full_string{t});
  print(" ");
  print(f->x);
  print(" ");
  print(f->s);
  print(" ");
  println(f);
}

t1{ptr{foo}}();
t1{cptr{foo}}();
t1{iptr{foo}}();
t1{tptr{foo}}();
t1{tcptr{foo}}();
