public namespace inc1;
public import common -;
public import meta m;

public struct foo {
  public int x;
}

public function {t} foo foo_inc__(foo mutable& v)
{
  ++v.x;
  return v;
}

public function {t} foo foo_dec__(foo mutable& v)
{
  --v.x;
  return v;
}

{
  foo v;
  ++v;
  println("v.x", v.x);
  --v;
  println("v.x", v.x);
}

