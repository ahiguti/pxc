public namespace splitkm;
import common -;

function {tkey, tmapped} tree_map{tkey, tmapped}
split_km(cslice{string} const& ss)
{
  tree_map{tkey, tmapped} r;
  for (const i, const s: ss) {
    tkey k;
    tmapped m;
    s.split{'='}(k, m);
    r[k] = m;
  }
  return r;
}

for (const k, const v: split_km{string, string}(environ)) {
  println(k, v);
}

