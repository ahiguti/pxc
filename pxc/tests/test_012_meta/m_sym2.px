namespace m_sym2;
import common -;
import meta m;

foo x;
show_symbol(x);

function {t} void show_symbol(t x) {
  metafunction sname m::concat{t, "_to_string"};
  metafunction sym m::symbol{sname, m::get_namespace{t}};
  if (m::eq{m::to_string{sym}, sname}) {
    println(sym(x));
  } else {
    println("func not found");
  }
}

