namespace ifblock;
import common -;

import meta m;

function {t} void test1() {
  if (m::eq{m::to_string{t}, "foo"}) {
    println("type foo");
  } else if (m::eq{m::to_string{t}, "bar"}) {
    println("type bar");
  } else {
    println("other type");
  }
}

struct foo { }
struct bar { }
struct baz { }

test1{foo}();
test1{bar}();
test1{baz}();

