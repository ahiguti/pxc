import common -;

namespace m_memofib;
import meta -;
import meta m;

struct {x} sfib {
  metafunction val
    cond{lt{x, 2}, x, add{m::symbol{sfib{sub{x, 2}}, "val"}, m::symbol{sfib{sub{x, 1}}, "val"}}};
}

// metafunction fib{x} cond{lt{x, 2}, x, add{fib{sub{x, 2}}, fib{sub{x, 1}}}};

metafunction fib{x} m::symbol{sfib{x}, "val"};

int v = fib{10};
println(v);

