
import io;

interface ifoo {
  function int get_x() const;
  function int get_y() const;
}

struct foo : ifoo : {
  int x;
  int y;
  function int get_x() const { return x; }
  function int get_y() const { return y; }
}

struct bar : ifoo : {
  int x;
  int y;
  int z;
  int w;
  function int get_x() const { return x; }
  function int get_y() const { return y; }
}

ref{ifoo} p1 = foo(5, 10);
println(p1->get_y());

ref{ifoo} p2 = bar(1, 2, 3, 4);
println(p2->get_y());

