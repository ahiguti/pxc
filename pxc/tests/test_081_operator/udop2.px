namespace udop2;
import operator -;
import common -;

struct foo {
  int x;
  int y;
}

function foo foo___add(foo const& v, foo const& w) {
  foo r;
  r.x = v.x + w.x;
  r.y = v.y + w.y;
  return r;
}
function void foo___shiftla(foo mutable& v, int w) {
  v.x <<= w;
  v.y <<= w;
}

{
  foo a;
  a.x = 10;
