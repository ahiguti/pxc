namespace udop;
import operator -;
import common -;

struct foo {
  int x;
  int y;
  function foo __add(foo const& v) const {
    foo r;
    r.x = x + v.x;
    r.y = y + v.y;
    return r;
  }
  function void __shiftla(int v) {
    x <<= v;
    y <<= v;
  }
}

{
  foo a;
  a.x = 10;
  a.y = 11;
  foo b;
  b.x = 3;
  b.y = 5;
  const c = a + b;
  println(c);
  b <<= 3;
  println(b);
}

