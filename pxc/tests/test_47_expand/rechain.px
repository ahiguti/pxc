namespace concat_argdecls;
import common -;
import meta::builtin m;

macro xy m::list{
  m::list{"x", 0},
  m::list{"y", 0}};


macro vw m::list{
  m::list{0, "v", string, 0, 0},
  m::list{0, "w", string, 0, 0}};

function void s5() {
  expand(s: xy) {
    println(m::to_string{s});
  }
  expand(s2: xy) {
    println(m::to_string{s2});
  }
}

function void s6() {
  expand(s: m::list) {
    println(m::to_string{s});
  }
  expand(s2: m::list) {
    println(m::to_string{s2});
  }
}

function void test1() {
  s5();
  s6();
}

test1();
