
namespace closure6;
import io;

function void test()
{
  int x = 0;
  function void foo(int v) {
    if (v <= 0) {
      return;
    }
    x += v;
    bar(v - 1);
  }
  function void bar(int v) {
    if (v <= 0) {
      return;
    }
    x += v;
    foo(v - 1);
  }
  foo(5);
  io::println(x);
}

test();

