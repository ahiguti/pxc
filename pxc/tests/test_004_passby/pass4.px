
public namespace pass4;
import common -;

threaded struct foo {
  public int value;
}

function int f1(foo const & x)
{
  return x.value + 1;
}

function foo f2(foo const x)
{
  foo y = x;
  y.value *= 2;
  return y;
}

function int test1(foo const x)
{
  return f1(f2(x));
}

foo f;
f.value = 5;
println(test1(f));

