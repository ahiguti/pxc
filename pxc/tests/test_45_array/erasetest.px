namespace erasetest;
import common -;
import text::conversion -;

function vector{string} create(cstrref const& prefix, size_t n)
{
  vector{string} v;
  for (size_t i : 0 .. n + 1) {
    string s = prefix;
    serialize_to_string(i, s);
    v.push_back(s);
  }
  return v;
}

function void t1()
{
  size_t n = 10;
  for (size_t i : 0 .. n) {
    for (size_t j : 0 .. n + 2) {
      vector{string} v = create("v", n - 1);
      v.erase(i, j);
      println(v);
    }
  }
}

t1();
