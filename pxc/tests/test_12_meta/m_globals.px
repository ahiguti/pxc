
namespace m_globals;
import meta;
import io;

function {t} int foo() {
  return 3;
}

macro funcs{ns} meta::metamap{meta::functions{ns}, meta::to_string};
macro types{ns} meta::metamap{meta::types{ns}, meta::to_string};
macro ns_funcs meta::metamap{meta::list{"io", "pxcrt", "m_globals"}, funcs};
macro ns_types meta::metamap{meta::list{"io", "pxcrt", "m_globals"}, types};
io::println(meta::concat{"FUNC: ",  meta::to_string{ns_funcs}});
io::println(meta::concat{"TYPE: ",  meta::to_string{ns_types}});

