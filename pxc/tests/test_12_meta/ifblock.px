namespace ifblock;
import test_common "";

import meta;
import io;

function {t} void test1() {
  if (meta::eq{meta::to_string{t}, "foo"}) {
    io::println("type foo");
  } else if (meta::eq{meta::to_string{t}, "bar"}) {
    io::println("type bar");
  } else {
    io::println("other type");
  }
}

struct foo { }
struct bar { }
struct baz { }

test1{foo}();
test1{bar}();
test1{baz}();

