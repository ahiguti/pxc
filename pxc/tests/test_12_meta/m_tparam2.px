import test_common "";

namespace m_tparam;
import meta::builtin m;

function {t, s} int foo() {
  return 3;
}

struct {t} bar {
  int x;
}

struct baz {
}

macro name_np{s}
  m::concat{"(", m::full_string{s}, ":", m::num_tparams{s}, ")"};
macro funcs{ns} m::metamap{m::functions{ns}, name_np};
macro types{ns} m::metamap{m::types{ns}, name_np};
macro ns_funcs funcs{"m_tparam"};
macro ns_types types{"m_tparam"};
println(m::concat{"FUNC: ",  m::full_string{ns_funcs}});
println(m::concat{"TYPE: ",  m::full_string{ns_types}});

macro inst foo{baz, baz};
println(m::concat{"INST: ",  m::full_string{name_np{inst}}});
println(m::full_string{int});
println(m::full_string{tree_map{int,int}});

