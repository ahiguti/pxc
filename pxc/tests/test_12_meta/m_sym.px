
namespace hoge;
import meta;
import io;

function {t} void show_symbol(t x) {
  macro sym meta::symbol{t, meta::concat{t, "_to_string"}};
  string s = meta::concat{t, ": ", sym};
  io::println(s);
}

function string foo_to_string(foo f) {
  return "foo_str";
}

struct foo {
}

foo x;
show_symbol(x);
