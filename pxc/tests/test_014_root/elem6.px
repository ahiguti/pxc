
public namespace elem6;
import common -;

struct foo {
  public int value;
}

function foo f1(vector{foo} mutable& v, foo mutable x) // passed by value
{
  println("a");
  v.resize(20, foo()); // ok
  println("b");
  x.value = 10;
  return x;
}

function void test1()
{
  vector{foo} v;
  v.resize(10, foo());
  v[3] = foo(10);
  foo z = f1(v, v[3]);
  v[3] = foo(5);
  println(v[3].value);
  println(z.value);
}

test1();
