public namespace erasetest;
import common -;
import text::string::serialize -;

function varray{string} create(cstrref const& prefix, size_t n)
{
  varray{string} v;
  for (size_t i : 0 .. n + 1) {
    string s = prefix;
    serialize_to_string(i, s);
    v.push_back(s);
  }
  return v;
}

function void t1()
{
  size_t n = 10;
  for (size_t i : 0 .. n) {
    for (size_t j : 0 .. n + 2) {
      varray{string} v = create("v", n - 1);
      const j1 = j <= n ? j : n;
      const i1 = i < j1 ? i : j1;
      v.erase(i1, j1);
      println(v);
    }
  }
}

t1();
