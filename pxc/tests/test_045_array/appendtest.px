public namespace appendtest;
import common -;
import text::string::serialize -;

function varray{string} create(size_t n)
{
  varray{string} v;
  for (size_t i : 0 .. n + 1) {
    string s = "v";
    serialize_to_string(i, s);
    v.push_back(s);
  }
  return v;
}

function void t1()
{
  size_t n = 10;
  for (size_t i : 0 .. n) {
    varray{string} v = create(n);
    varray{string} p = create(i);
    v.append(p);
    println(v);
  }
}

t1();
