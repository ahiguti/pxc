public namespace estl;
import common -;
import meta m;

function int sum(int x, int y, int z)
{
  return x + y + z;
}

function int dbl(int x)
{
  return x * 2;
}

metafunction L m::list;

function void test1(string const s) {
  int a = 1;
  int b = 2;
  int c = 3;
  int r = 0;
  expand(fsym : m::list{m::list{"sum", 0}, m::list{"dbl", 1}}) {
    if (s == m::to_string{fsym}) {
      r = fsym(a, b, c);
    }
    if (s == m::to_string{fsym}) {
      r = fsym(a);
    }
  }
  println(r);
}

test1("sum");
test1("dbl");

