
namespace deref3;
import common -;

metafunction tm tree_map{int, int};
metafunction tmr tree_map_crange{int, int};

function int t1(tmr const& ra)
{
  var v = *ra;
  v *= 2;
  return v;
}

function tm f1()
{
  tree_map{int, int} m;
  m[3] = 55;
  return m;
}

function void test()
{
  int v = t1(f1());
  println(v);
}

test();
