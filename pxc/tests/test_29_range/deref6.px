
namespace deref1;
import common -;

function int foo(int v)
{
  return v + v;
}

function int t1(tree_map{int,int} & m, int key)
{
  int v = foo(m[key]);
    /* arg is passed by value. returning-value expr must be emitted for
     * 'm[key]' */
  v *= 2;
  return v;
}

function void test()
{
  tree_map{int, int} m;
  m[3] = 55;
  int v = t1(m, 3);
  println(v);
}

test();
