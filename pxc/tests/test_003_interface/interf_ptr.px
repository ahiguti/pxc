import common -;

namespace interf_ptr;

interface ifoo {
  function void m1() const;
}

struct foo < ifoo > {
  function void m1() const {
    println("m1");
  }
}

function void test2(ptr{ifoo} x)
{
  x->m1();
}

function void test1()
{
  ptr{foo} f = make_ptr{foo}(foo());
  test2(f);
}

test1();
