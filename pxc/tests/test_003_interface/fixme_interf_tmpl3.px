import common -;

namespace interf_tmpl3;

interface ifoo {
  function int m(int x);
}

struct {t} foo < ifoo > {
  function int m(int x) {
    return data + x;
  }
  t data;
}

function void test1()
{
  int q = 10;
  ptr{foo{int}} r = make_ptr{foo{int}}(foo(q));
  ptr{ifoo} ir = r;
  int v = ir->m(100);
  println(v);
}

test1();

