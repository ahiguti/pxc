namespace mtchk2;
import common -;
import thread -;

threaded struct foo {
  int x;
}

multithreaded struct bar {
  function int m1() {
    foo f; // can use non-multithreaded type inside a member function
    f.x = 1;
    return f.x;
  }
}

function void t1() {
  bar b;
  int x = b.m1();
  println(x);
}

t1();

