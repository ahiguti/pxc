{
  v.push_back(x);
}
template <typename T> T
pop_back(pxcrt::pxcvarray<T>& v)
{
  if (v.empty()) {
    T rv = T();
    return rv;
  } else {
    T rv = v[v.size() - 1];
    v.pop_back();
    return rv;
  }
}
