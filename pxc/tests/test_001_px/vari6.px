public namespace vari6;
import common -;

threaded union foo {
  public int intval;
  public string strval;
}

function void println_foo(foo v)
{
  if (case v.intval) {
    println(v.intval);
  } else if (case v.strval) {
    println(v.strval);
  }
}

function bool hoge(bool v)
{
  return v;
}

foo v;
v.intval = 3;
bool b = hoge(case v.intval);
if (b) {
  println("true");
} else {
  println("false");
}
