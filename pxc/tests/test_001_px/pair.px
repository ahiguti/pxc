import common -;

public namespace pair;

public struct {T} pair {
  public T first;
  public T second;
  public function T rec(T x, T i)
  {
    if (i == 0) { return x; }
    return rec(x, i - 1) + rec(x, i - 1);
  }
  public function T get()
  {
    return rec(first, 10) + rec(second, 10);
  }
}

