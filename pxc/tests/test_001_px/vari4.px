public namespace vari4;
import common -;

threaded union foo {
  public int intval;
  public string strval;
}

function void println_foo(foo v)
{
  if (case v.intval) {
    println(v.intval);
  } else if (case v.strval) {
    println(v.strval);
  }
}

foo v;
v.intval = 3;
println_foo(v);
v.strval = "hoge";
println_foo(v);

