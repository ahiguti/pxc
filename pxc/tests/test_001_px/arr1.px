function extern "array::push_back" {t}
  void push_back(vector{t} mutable& vec, t const& val);
function extern "array::pop_back" {t}
  t pop_back(vector{t} mutable& vec);

extern "implementation" inline
namespace array {
template <typename T> void
push_back(pxcrt::pxcvector<T>& v, const T& x)
{
  v.push_back(x);
}
template <typename T> T
pop_back(pxcrt::pxcvector<T>& v)
{
  if (v.empty()) {
    T rv = T();
    return rv;
  } else {
    T rv = v[v.size() - 1];
    v.pop_back();
    return rv;
  }
}
}; // namespace array
