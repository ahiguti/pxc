import common -;

import array;
public namespace erat2;

function ptr{varray{uint}} eratosthenes(int x)
{
  ptr{varray{uint}} ret;
  varray{uint} sieve;
//  array::resize(sieve, x);
// error!
  sieve[1] = 1;
  for (uint i = 2; i < x; ++i) {
    if (sieve[i] == 0) {
      array::push(*ret, i);
      for (uint j = i * i; j < x; j += i) {
	sieve[j] = 1;
      }
    }
  }
  return ret;
}

ptr{varray{uint}} arr = eratosthenes(100000);
for (uint i = 0; i < array::length(*arr); ++i) {
  println((*arr)[i]);
}

