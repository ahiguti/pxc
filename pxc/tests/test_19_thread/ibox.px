
import io;

interface ifoo {
  function int get_x() const;
  function int get_y() const;
}

struct foo : ifoo : {
  int x;
  int y;
  function int get_x() const { return x; }
  function int get_y() const { return y; }
}

struct bar : ifoo : {
  int x;
  int y;
  int z;
  int w;
  function int get_x() const { return x; }
  function int get_y() const { return y; }
}

function void test1()
{
  ref{ifoo} p1 = ref(foo(5, 10));
  io::main.println(p1->get_y());
  ref{ifoo} p2 = ref(bar(1, 2, 3, 4));
  io::main.println(p2->get_y());
}

test1();
