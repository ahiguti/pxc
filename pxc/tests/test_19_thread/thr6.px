import test_common "";

namespace ttest;
import thread "";

multithreaded struct my_thread_obj (io::ioptr sys0) {
  int x;
  int y;
  io::ioptr sys = sys0;
  function void main() {
    io::standard::stdout_println(sys, "my thread main");
    io::standard::stdout_println(sys, x);
    io::standard::stdout_println(sys, y);
  }
}

function void test1()
{
  threadptr thr = thread_create(my_thread_obj());
  thr.join();
}

test1();

