namespace ptrpass;
import test_common "";

struct foo {
  int x;
  int y;
}

function int foo_ptrarg(ptr{foo} p)
{
  return p->x + p->y;
}

function int pass_foo()
{
  ptr{foo} p = ptr{foo}(foo(10, 20));
  return foo_ptrarg(p);
}

int x = pass_foo();
io::standard::stdout_println(io::system::system, x);

