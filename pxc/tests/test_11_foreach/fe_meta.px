namespace fe_meta;
import common -;
import meta::builtin m;


function {t} void field_to_string(string n, t fld, string& str_r)
{
  str_r.append("(");
  str_r.append(n);
  str_r.append(" : ");
  serialization::string::serialize_to_string(fld, str_r);
  //m::symbol{t, m::concat{t, "_serialize_to_string"}}(fld, str_r);
  str_r.append(")");
}

struct foo {
  int x;
  long y;
}

function string test()
{
  foo v;
  v.x = 100;
  v.y = 200;
  string s;
  foreach (name, sym : m::list{"y", "y", "x"}) {
    field_to_string(name, v.sym, s);
  }
  return s;
}

string s = test();
println(s);

