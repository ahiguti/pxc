function {t} void field_to_string(string n, t fld, string mutable& str_r)
{
  str_r.append("(");
  str_r.append(n);
  str_r.append(" : ");
  str_r.append(meta::symbol{t, meta::concat{t, "_to_string"}}(fld));
  str_r.append(")");
}

