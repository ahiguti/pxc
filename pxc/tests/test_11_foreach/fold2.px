namespace fold2;
import common -;
import serialization::positional -;
import meta::builtin m;

function int foo(int v, int x, int y, int z, int w)
{
  return v + x + y + z + w;
}

function int foostr(string x, string y, string z)
{
  int r;
  foreach (i : m::argnames{foostr};
    j : decimal_to_integral_nocheck{int}(i); ",") {
    r = foo(2, j, 6);
  }
  return r;
}

println(foostr("3", "4", "5"));

