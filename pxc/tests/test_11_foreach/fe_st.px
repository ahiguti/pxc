namespace fe_st;
import common -;
import meta::common m;


function {t} void field_to_string(t fld, string& str_r)
{
  str_r.append(" ");
  string::conversion::serialize_to_string(fld, str_r);
  // str_r.append(m::symbol{t, m::concat{t, "_to_string"}}(fld));
}

struct foo {
  int x;
  long y;
}

function void test()
{
  foo f;
  f.x = 20;
  f.y = 35;
  int i;
  string s = "(";
  expand (fld : m::field_names{foo}) {
    field_to_string(f.fld, s);
  }
  s.append(" )");
  println(s);
}

test();

