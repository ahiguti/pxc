}

private metafunction gl_enumval
  m::join{
    GL_ES_VERSION_2_0_ENUMVAL,
    GL_ES_VERSION_3_0_ENUMVAL};
private metafunction gl_bfval
  m::join{
    GL_ES_VERSION_2_0_BFVAL,
    GL_ES_VERSION_3_0_BFVAL};
