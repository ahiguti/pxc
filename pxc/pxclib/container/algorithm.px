namespace container::algorithm;
public import type::builtin "";

public threaded function {t} void reverse(slice{t} const& x)
{
  macro size_type size_t;
  macro mapped_type slice{t}::mapped_type;
  size_type s = 0;
  size_type f = x.size();
  while (s < f) {
    --f;
    mapped_type const v = x[s];
    x[s] = x[f];
    x[f] = v;
    ++s;
  }
}

