namespace meta::vararg;
public import meta::common m;

public metafunction generate_argdecls{types}
  m::map{
    m::seq{m::size{types}},
    metafunction{i} m::list{ m::concat{"a", i}, m::at{types, i}, 0, 1}};
public metafunction generate_arg_names{sz}
  m::map{m::seq{sz}, metafunction{x} m::concat{"a", x}};
public metafunction generate_field_names{sz}
  m::map{m::seq{sz}, metafunction{x} m::concat{"_", x}};

