#!/usr/bin/pxc

namespace fib;
import common -;

function {t} t fib(t x) {
  return x <= 1 ? x : fib(x - 2) + fib(x - 1);
}

int x = argv.size() > 0 ? type::builtin::string_to_bt_int(argv[0]) : 40;
int n = argv.size() > 1 ? type::builtin::string_to_bt_int(argv[1]) : 1;
int i;
for (i = 0; i < n; ++i) {
  println(fib(x));
}

