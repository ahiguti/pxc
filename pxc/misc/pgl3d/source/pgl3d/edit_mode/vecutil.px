private threaded namespace pgl3d::edit_mode::vecutil "use-unsafe";
public import common -;
public import meta m;
public import glm -;

public metafunction ui static_cast{uint};
public metafunction si static_cast{int};

public threaded function bool lt_all_uvec3(uvec3 const& a, uvec3 const& b)
{
  return a.x < b.x && a.y < b.y && a.z < b.z;
}

public threaded function bool le_all_uvec3(uvec3 const& a, uvec3 const& b)
{
  return a.x <= b.x && a.y <= b.y && a.z <= b.z;
}

